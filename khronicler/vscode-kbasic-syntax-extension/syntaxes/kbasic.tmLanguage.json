{
  "scopeName": "source.kbasic",
  "name": "KBasic Language",
  "patterns": [
    {
      "name": "keyword.control.kbasic",
      "match": "\\b(dim|redim|goto|if|elseif|else|while|do|for|to|step|next|continue|break|end|return|func|exit)\\b"
    },
    {
      "name": "string.quoted.double.kbasic",
      "match": "\"[^\"]*\""
    },
    {
      "name": "constant.numeric.kbasic",
      "match": "\\b\\d+\\b"
    },
    {
      "name": "comment.line.kbasic",
      "match": "#.*$"
    }
  ],
  "repository": {}
}